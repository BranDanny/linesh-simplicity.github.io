{
  "name": "linesh-blog",
  "author": "congyu-lin_linesh",
  "version": "2.0.0",
  "description": "Personal blog with technical and philosophical meditation",
  "main": "index.html",
  "keywords": [
    "blog",
    "linesh",
    "spring",
    "angular"
  ],
  "dependencies": {
    "angular-cache": "^4.5.0",
    "highlight.js": "^9.3.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0"
  },
  "devDependencies": {
    "angular": "^1.5.5",
    "angular-marked": "^1.0.1",
    "angular-mocks": "^1.5.5",
    "angular-route": "^1.5.5",
    "angular-utf8-base64": "0.0.2",
    "babel": "^6.5.2",
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.5.0",
    "github-api": "^0.11.2",
    "html-webpack-plugin": "^2.24.1",
    "json-stable-stringify": "^1.0.1",
    "microlight": "0.0.1",
    "ng-annotate-webpack-plugin": "^0.1.2",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "scripts": {
    "build": "node hooks/precommit-generate-post-json.js && webpack --devtool cheap-module-source-map --progress --optimize-minimize",
    "build:jk": "bundle exec jekyll build --incremental",
    "watch": "webpack --watch -d -p",
    "start": "webpack-dev-server --devtool cheap-module-source-map --optimize-minimize --profile --port 7000",
    "serve:jk": "bundle exec jekyll serve --watch --incremental --port 7001",
    "start:react": "webpack-dev-server --devtool cheap-module-source-map --optimize-minimize --profile --config webpack.react.config.js --port 8000"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/linesh-simplicity/linesh-simplicity.github.io.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/linesh-simplicity/linesh-simplicity.github.io/issues"
  },
  "homepage": "https://github.com/linesh-simplicity/linesh-simplicity.github.io#readme"
}
